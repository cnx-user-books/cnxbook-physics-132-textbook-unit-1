                  <m:mi>U</m:mi>
                </m:mrow>
                <m:mi>f</m:mi>
              </m:msub>
            </m:mrow>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0">{K} rsub {i} {+U} rsub {i} = {{K} rsub {f} +U} rsub {f}</m:annotation>
