            </m:mrow>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0">{1} over {2} m {v} ^ {2} {+ mgh} rsub {i} = {0 +mgh} rsub {f}</m:annotation>
        </m:semantics>
      </m:math>
    </equation>
