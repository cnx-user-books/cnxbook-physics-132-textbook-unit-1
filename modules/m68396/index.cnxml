            <m:msub>
              <m:mi>E</m:mi>
              <m:mi>f</m:mi>
            </m:msub>
          </m:mrow>
          <m:annotation encoding="StarMath 5.0">{E} rsub {i} = {E} rsub {f}</m:annotation>
