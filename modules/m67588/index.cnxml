         </m:msup>
        </m:mrow>
       </m:mrow>

      <m:mrow/>
     </m:mrow>
     <m:annotation encoding="StarMath 5.0">
size 12{0 "." 1`"W/m" rSup { size 8{2} } } {}
     </m:annotation>
